generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL_TEST")
}

model User {
  id        String    @id @default(uuid())
  name      String
  login     String    @unique
  password  String
  createdAt DateTime  @default(now())
  updatedAt DateTime? @updatedAt
}

model Nfe {
  id            String    @id @default(uuid())
  numberNf      Int
  series        Int
  urlDanfe      String?
  chaveNfe      String
  emissionDate  DateTime
  recipientCNPJ String
  recipientName String
  totValue      Decimal
  totICMS       Decimal
  createdAt     DateTime  @default(now())
  updatedAt     DateTime? @updatedAt
}
